            url: y
        });
        // Save all photos for later viewing
        this.storage.set('back', this.photosB);
    }
    loadSaved() {
        this.storage.get('front').then((photos) => {
            this.photosF = photos || [];
        });
        this.storage.get('back').then((photos) => {
            this.photosB = photos || [];
        });
    }
